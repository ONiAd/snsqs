{
    "name": "enqueue/snsqs",
    "type": "library",
    "description": "Message Queue Amazon SNS-SQS Transport",
    "keywords": ["messaging", "queue", "amazon", "aws", "sns", "sqs"],
    "homepage": "https://enqueue.forma-pro.com/",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "queue-interop/queue-interop": "^0.7|^0.8",
        "enqueue/dsn": "^0.9",
        "enqueue/sns": "dev-master",
        "enqueue/sqs": "^0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4.0",
        "enqueue/test": "0.9.x-dev",
        "queue-interop/queue-spec": "^0.6"
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/uro/sns"
        }
    ],
    "support": {
        "email": "opensource@forma-pro.com",
        "issues": "https://github.com/php-enqueue/enqueue-dev/issues",
        "forum": "https://gitter.im/php-enqueue/Lobby",
        "source": "https://github.com/php-enqueue/enqueue-dev",
        "docs": "https://github.com/php-enqueue/enqueue-dev/blob/master/docs/index.md"
    },
    "autoload": {
        "psr-4": { "Enqueue\\SnsQs\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    }
}
